# Keyless AI Platform Configuration (Solana-based)

# Server Configuration
PORT=3000
NODE_ENV=development

# Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h
SESSION_SECRET=your-session-secret-here
BCRYPT_ROUNDS=12

# API Configuration
API_KEY_HEADER=X-Wallet-Address
MAX_REQUESTS_PER_MINUTE=100
CORS_ORIGIN=*

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_NETWORK=mainnet-beta
KEY_TOKEN_MINT=your-key-token-mint-address-here
USDC_MINT=usd-key-here
REWARDS_TREASURY_WALLET=your-rewards-treasury-wallet-address-here
PAYMENT_PROGRAM_ID=your-payment-program-id-here
JUPITER_API_URL=https://quote-api.jup.ag/v6
POINTS_PRICE_USDC=0.001

# AI Service Models
TEXT_GENERATION_MODEL=gpt-4
IMAGE_GENERATION_MODEL=dall-e-3
CODE_GENERATION_MODEL=gpt-4
AUDIO_GENERATION_MODEL=whisper
DATA_ANALYSIS_SERVICE=internal
SEARCH_SERVICE=internal

# Database (for production)
# DATABASE_URL=postgresql://username:password@localhost:5432/keyless
# REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info
LOG_FILE=logs/keyless-backend.log

# External Services
# SENTRY_DSN=your-sentry-dsn-here
# OPENAI_API_KEY=your-openai-api-key-here
# STABILITY_AI_API_KEY=your-stability-ai-key-here
