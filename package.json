{
  "name": "@keyless-app/keyless-backend",
  "version": "1.0.3",
  "description": "Solana-powered AI generation platform with Revenue-to-Buyback Flywheel. The first AI platform with real-time, transparent buyback mechanism powered by Solana's speed and low fees.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "node scripts/clean.js",
    "example": "ts-node examples/basic-usage.ts",
    "example:build": "tsc examples/basic-usage.ts --outDir dist/examples --target es2020 --module commonjs",
    "example:api": "node examples/api-usage.js",
    "dev:watch": "nodemon src/index.ts"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "ai-generation",
    "machine-learning",
    "solana",
    "blockchain",
    "spl-token",
    "usdc",
    "jupiter",
    "dex",
    "buyback",
    "flywheel",
    "points-system",
    "text-generation",
    "image-generation",
    "code-generation",
    "audio-generation",
    "keyless",
    "api",
    "rest-api",
    "web3",
    "crypto",
    "token"
  ],
  "author": "Keyless Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keyless-app/keyless-backend.git"
  },
  "bugs": {
    "url": "https://github.com/keyless-app/keyless-backend/issues"
  },
  "homepage": "https://github.com/keyless-app/keyless-backend#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "zod": "^3.22.4",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
